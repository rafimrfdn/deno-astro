{
  "name": "",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "deno run -A npm:astro dev",
    "start": "deno run -A npm:astro dev",
    "build": "deno run -A npm:astro check && deno run -A npm:astro build",
    "preview": "deno run -A ./dist/server/entry.mjs",
    "astro": "astro",
    "format": "deno fmt && prettier --write ."
  },
  "dependencies": {
    "sass": "^1.77.6",
    "@astrojs/deno": "^4.2.0",
    "astro": "^2.8.3",
    "deno-astro-adapter": "^4.2.0",
    "@astrojs/check": "^0.5.10",
    "typescript": "^5.4.3",
    "@fontsource-variable/ballet": "^5.0.18",
    "@fontsource-variable/cinzel": "^5.0.8",
    "@fontsource-variable/inter": "^5.0.16",
    "@fontsource-variable/orbitron": "^5.0.18",
    "@fontsource-variable/raleway": "^5.0.17",
    "@fontsource/ephesis": "^5.0.18",
    "@fontsource/gloock": "^5.0.18",
    "@fontsource/italiana": "^5.0.18",
    "@fontsource/lato": "^5.0.18",
    "@fontsource/poppins": "^5.0.8",
    "@fontsource/windsong": "^5.0.18",
    "@astrojs/solid-js": "^3.0.3",
    "solid-js": "^1.8.7",
    "@astrojs/svelte": "^2.1.0",
    "svelte": "^3.56.0"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.11.0"
  },
  "prettier": {
    "tabWidth": 2,
    "plugins": [
      "./node_modules/prettier-plugin-astro/dist/index.js"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
}
